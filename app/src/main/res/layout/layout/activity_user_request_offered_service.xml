<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbg"
    tools:context=".ClientSide.UserRequestOfferedService">

    <View
        android:id="@+id/topview"
        android:layout_width="414dp"
        android:layout_height="74dp"
        android:alpha="0.7"
        android:background="#33213A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="116dp"
        android:layout_height="96dp"
        android:src="@drawable/png"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="84dp"

        android:text="@string/sreq"
        android:textColor="#F8F5F5"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/topview" />

    <View
        android:id="@+id/topview2"
        android:layout_width="380dp"
        android:layout_height="580dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:alpha="0.45"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topview" />

    <TextView
        android:id="@+id/SerType"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView38"
        app:layout_constraintStart_toStartOf="@+id/abtService" />

    <TextView
        android:id="@+id/Ex_issue"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView50"
        tools:layout_editor_absoluteX="225dp" />

    <TextView
        android:id="@+id/ContacNum"
        android:layout_width="154dp"
        android:layout_height="25dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/cancel_action"
        app:layout_constraintStart_toStartOf="@+id/Ex_issue" />

    <TextView
        android:id="@+id/CustNmae"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView47"
        app:layout_constraintStart_toStartOf="@+id/ContacNum" />

    <TextView
        android:id="@+id/Prange"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView40"
        app:layout_constraintStart_toStartOf="@+id/CustNmae" />

    <TextView
        android:id="@+id/EstimaetdDel"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView21"
        app:layout_constraintStart_toStartOf="@+id/Prange" />


    <TextView
        android:id="@+id/abtService"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView45"
        app:layout_constraintStart_toStartOf="@+id/EstimaetdDel" />

    <TextView
        android:id="@+id/Techname"
        android:layout_width="181dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="36dp"
        android:text="@string/techname"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toBottomOf="@+id/topview" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="181dp"
        android:layout_height="30dp"
        android:layout_marginTop="64dp"
        android:text="@string/Praneg"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView47"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="181dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:text="@string/AboutSrvice"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="181dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:text="@string/Eta"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="177dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:text="@string/CustName"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/cancel_action"
        app:layout_constraintTop_toBottomOf="@+id/textView51" />

    <TextView
        android:id="@+id/cancel_action"
        android:layout_width="175dp"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:text="@string/ConNum"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView50"
        app:layout_constraintTop_toBottomOf="@+id/textView47" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="@string/Issue"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cancel_action" />

    <Button
        android:id="@+id/SaveButton"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="112dp"
        android:background="@drawable/button_default"
        android:text="@string/submit"
        android:textColor="#ffff"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView50" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="136dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="136dp"
        android:text="@string/SRequest"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView40" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <TextView
        android:id="@+id/TechName"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:background="@drawable/textbox"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Techname"
        app:layout_constraintStart_toStartOf="@+id/SerType" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="181dp"
        android:layout_height="30dp"
        android:layout_marginTop="12dp"
        android:text="@string/SerType"
        android:textColor="#FFFFFF"

        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView45"
        app:layout_constraintTop_toBottomOf="@+id/Techname" />

</androidx.constraintlayout.widget.ConstraintLayout>